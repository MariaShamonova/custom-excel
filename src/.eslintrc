{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "quotes": ["warn", "single"],
    "indent": ["warn", 2],
    "object-curly-spacing": ["warn", "always"],
    "comma-dangle": ["error", "never"],
    "require-jsdoc": "off"
  }
}
